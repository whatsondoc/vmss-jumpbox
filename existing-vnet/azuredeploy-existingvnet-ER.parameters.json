{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmssSku": {
      "value": "<<compute_SKU>>"
    },
    "jumpboxSku": {
      "value": "<<jumpbox_SKU>>"
    },
    "CentOSversion": {
      "value": "<<OS_version>>"
