{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageAccountType": {
      "value": "<<_storage_account_type_>>"
    },
    "vmssSku": {
      "value": "<<_compute_SKU_>>"
    },
    "jumpboxSku": {
      "value": "<<_jumpbox_SKU_>>"
    },
    "CentOSversion": {
      "value": "<<_OS_version_>>"
    },
    "vmssName": {
      "value": "<<_vmss_name_>>"
    },
    "instanceCount": {
      "value": <<_instance_number_>>
    },
    "adminUsername": {
      "value": "<<_username_>>"
    },
    "adminPassword": {
      "value": "<<_password_>>"
    },
    "vnetName": {
        "value": "<<_vnet_name_>>"
    },
    "vnetResourceGroup": {
        "value": "<<_RG_name_>>"
    },
    "subnetName": {
        "value": "<<_subnet_name_>>"
    },
    "addressPrefix": {
      "value": "<<_vnet_IP_prefix_>>"
    },
    "subnetPrefix": {
      "value": "<<_subnet_IP_prefix_>>"
    },
  }
}
